  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "body-parser": "^1.17.2",
    "express-handlebars": "^5.3.1",
    "nodemailer": "^4.1.0"
  }
}
